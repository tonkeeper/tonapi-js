        "patch-package": "^8.0.0",
        "rimraf": "^5.0.7",
        "swagger-typescript-api": "13.0.9",
        "ts-jest": "^29.1.4",
        "ts-node": "^10.9.2",
        "tsx": "^4.16.2",
        "typescript": "^5.4.5"
